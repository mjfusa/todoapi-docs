{
  "metadata": [
    {
      "src": [
        {
          "src": ".",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "TodoOpenAPI",
      "_appTitle": "TodoOpenAPI",
      "_enableSearch": false,
      "pdf": false
    },
    "sitemap": {
      "baseUrl": "https://salmon-cliff-04321e61e.5.azurestaticapps.net/",
      "priority": 0.1,
      "changefreq": "monthly"
    }
  }
}